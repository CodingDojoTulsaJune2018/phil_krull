<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
    <h1>Recently added book</h1>
    <table>
        <tr>
            <td>Title</td>
            <td>Author</td>
        </tr>
        @{
            foreach(var book in @Model)
            {
                <tr>
                    <td>@book.Title</td>
                    <td>@book.Author</td>
                </tr>

            }
        }
    </table>
    <a href="/">Home</a>
</body>
</html>